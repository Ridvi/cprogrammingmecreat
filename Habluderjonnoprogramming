function jogkor (first,second){

  var jogfol=first+second;
  return jogfol;
  
 } 
var calerdam=345;
var aterdam=435;
console.log(jogkor(calerdam,aterdam));
var peyeci=jogkor(calerdam,aterdam);
console.log(peyeci);

