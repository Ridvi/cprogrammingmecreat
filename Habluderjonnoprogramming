        , "pabina"];
for(var i=0;i<=ara.length;i++){
  var agirl=ara[i];
  
 console.log(agirl+" You are beautiful"
           + " really");

  }




//more codes
function secondtoHour(seconds)
{
  var minutes=seconds/60;
  var hours=minutes/60;
  return hours;
  }
var sec=7200;
console.log(secondtoHour(sec));



