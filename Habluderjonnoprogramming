// level code here, look bellow
function jogkor (first,second){

  var jogfol=first+second;
  return jogfol;
  
 } 
function duigunkor(shongka)
{
  var goonfol=shongka*2;
  return goonfol;
  
  }
function dhardey(dharAmount)
{
  var friendertaka=dharAmount;
  return friendertaka;
  }
var calerdam=38;
var dalerdam=56;

var total=jogkor(calerdam,dalerdam);
var dharlagbe=duigunkor(total);
var dharpaisi=dhardey(dharlagbe);
console.log(dharpaisi);

//more code bellow
function jogkor (first,second){

  var jogfol=first+second;
  return jogfol;
  
 } 
function duigunkor(shongka)
{
  var goonfol=shongka*2;
  return goonfol;
  
  }
function dhardey(dharAmount)
{
  var friendertaka=dharAmount;
  return friendertaka;
  }



var calerdam=345;

var dalerdam=456;


function takadorker(first,second)
{
  var total=jogkor(first,second);
  var dharlagbe=duigunkor(total);
  var takapaici=dhardey(dharlagbe);
  return takapaici;
}

console.log(takadorker(
  calerdam,dalerdam));

